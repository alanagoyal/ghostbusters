# ============================================
# IMPORTANT: This is an example .env file
# Copy to .env and replace with your own values
# NEVER commit real credentials to version control
# ============================================

# DoorBird Camera Configuration
DOORBIRD_IP=your_doorbird_ip
DOORBIRD_USERNAME=your_doorbird_username
DOORBIRD_PASSWORD=your_doorbird_password

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# SSH Configuration
HOSTNAME=your_hostname
USERNAME=your_username
PASSWORD=your_password
NETWORK_NAME=your_network_name
NETWORK_PASSWORD=your_network_password

# GitHub Configuration
GITHUB_ACCESS_TOKEN=your_github_access_token

# Baseten Configuration
BASETEN_API_KEY=your_baseten_api_key
BASETEN_MODEL_ID=your_baseten_model_id
BASETEN_MODEL_URL=your_baseten_model_url

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key