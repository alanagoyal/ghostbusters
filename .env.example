# DoorBird Camera Configuration
# Copy this file to .env and fill in your actual values
# NEVER commit the .env file with real credentials!

# DoorBird connection details
DOORBIRD_IP=192.168.x.x
DOORBIRD_USERNAME=your_doorbird_username
DOORBIRD_PASSWORD=your_doorbird_password

# Baseten API for costume classification (add when ready)
BASETEN_API_KEY=your_baseten_api_key

# Supabase configuration
# Note: NEXT_PUBLIC_* vars are for frontend, SERVICE_ROLE_KEY is for backend/Raspberry Pi
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key
DEVICE_ID=halloween-pi
